{
    "manifest_version": 3,
    "name": "cuddly-octo-doodle",
    "description": "browsing session manager chrome extention",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/lucy.png"
    },
    "permissions": [
        "tabGroups",
        "bookmarks",
        "tabs"
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
    
}