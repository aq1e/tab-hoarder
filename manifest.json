{
    "manifest_version": 3,
    "name": "Tab Hoarder",
    "description": "Effortlessly store, manage, and restore tabs you are hoarding.",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/lucy.png"
    },
    "permissions": [
        "tabGroups",
        "bookmarks",
        "tabs"
    ]
}